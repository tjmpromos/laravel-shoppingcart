{
    "name": "tjmpromos/laravel-shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        },
        {
            "name": "Olly Warren",
            "email": "olly@ollywarren.com"
        },
        {
            "name": "Mike Wall",
            "email": "daikazu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/session": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/events": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "php-units-of-measure/php-units-of-measure": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0|~7.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Tjmpromos\\Laravel\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tjmpromos\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Tjmpromos\\Laravel\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Tjmpromos\\Laravel\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
